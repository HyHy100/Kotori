OUTPUT_ARCH( "riscv" )
ENTRY( _start )
SECTIONS
{
  /* text: test code section */
  /* . = 0x80000000; */
  . = 0x80000000;
  .text : { *(.text) }
  /* data: Initialized data segment */
  .data : { *(.data) }
  .rodata : { *(.rodata) }
  .sdata : { *(.sdata) }
  .htif : {
    . = ALIGN(8); 
    *(.htif)
  }

  .debug : { *(.debug) }
  . += 0x8000;
  stack_top = .;

  /* End of uninitalized data segement */
  _end = .;
}